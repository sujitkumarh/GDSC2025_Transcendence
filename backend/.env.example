# AWS Mistral AI Configuration
AWS_REGION=us-east-1
AWS_MISTRAL_MODEL=mistral.mistral-7b-instruct-v0:2
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
AWS_SESSION_TOKEN=optional_session_token_here

# Development Settings
MOCK_MODE=true
DEBUG=true
LOG_LEVEL=INFO
TELEMETRY_ENABLED=true

# Server Configuration
HOST=0.0.0.0
PORT=8000
CORS_ORIGINS=http://localhost:5173

# Database Configuration (for future expansion)
DATABASE_URL=sqlite:///./transcendence.db
DATA_DIR=./app/data

# Security Settings (for production)
SECRET_KEY=your_secret_key_for_jwt_signing
TOKEN_EXPIRE_MINUTES=60

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Caching
CACHE_TTL=3600
CACHE_MAX_SIZE=1000